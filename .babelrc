{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "last 2 Opera versions",
            "last 2 Chrome versions",
            "last 2 Firefox versions",
            "Safari >= 10"
          ]
        },
        "modules": false,
        "loose": true,
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ]
}
